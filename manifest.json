{
  "manifest_version": 3,
  "name": "Chrome Worker",
  "description": "A non-intrusive, modern and clean dashboard for productive keep-tracking.",
  "version": "0.1.0",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "chrome_url_overrides": {
    "newtab": "dashboard/index.html"
  }
}
